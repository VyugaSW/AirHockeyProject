<Window x:Class="AirHockeyProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AirHockeyProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="540" Width="520">
    <Canvas>
        <Rectangle Width="515" Height="515" Stroke="Black" StrokeThickness="2"/>
        <Ellipse x:Name="ellipse" Fill="Red" Width="20" Height="20"/>
        <Ellipse x:Name="ellipsePuck" Fill="Blue" Width="20" Height="20" Canvas.Left="50" Canvas.Top="50"/>
    </Canvas>
</Window>
